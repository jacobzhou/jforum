<%= form_for @survey_user_sheet, :html => {:multipart => true} do |f| %>
  <%= hidden_field_tag 'survey_id', @survey.id %>
  <% @survey_questions.each_with_index do |survey_quetion, index| 
    survey_user_answer = @survey_user_sheet.survey_user_answer(survey_quetion.id)
  %>
    <%= question_html(survey_quetion, survey_user_answer, index + 1) %>
  <% end -%>
  <div class="tc m12"><%= f.submit "提交答卷", :class => "sheet_submit btn btn-primary" %></div>
<% end -%>