<div class="box">
  <div class="cell">
    <div class="fr">
      <%= link_to '调查问卷', admin_surveys_path, :class => "btn btn-small" %>
      <%#= admin_create_button('调查答卷', :survey_user_sheet) %>
    </div>
    <%= build_admin_navigation(['调查答卷']) %>
  </div>
  <div class="cell">
    <table class="table">
      <thead>
        <tr>
          <th>序号</th>
          <th>调查问卷</th>
          <th>答卷时间</th>
          <th>操作</th>
        </tr>
      </thead>
      <% @survey_user_sheets.each_with_index do |survey_user_sheet, index| %>
        <tr>
          <td><%= index_no(index, @survey_user_sheets.size) %></td>
          <td><%= survey_user_sheet.survey.name %></td>
          <td><%= survey_user_sheet.created_at %></td>
          <td><%= link_to "修改", edit_survey_user_sheet_path(survey_user_sheet) %>
              <%= link_to "删除", survey_user_sheet, :method => :delete, :confirm => "您确定吗？" %>
          </td>
        </tr> 
      <% end -%>
    </table>
    <div class="inner">
      <%= paginate @survey_user_sheets %>
    </div>
  </div>
</div>